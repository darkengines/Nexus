<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>install</servlet-name>
        <servlet-class>darkengines.conference.web.Install</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>join</servlet-name>
        <servlet-class>darkengines.conference.web.Join</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>darkengines.conference.web.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>WebSocket</servlet-name>
        <servlet-class>darkengines.conference.web.WebSocket</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EmailAvailable</servlet-name>
        <servlet-class>darkengines.conference.web.EmailAvailable</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>install</servlet-name>
        <url-pattern>/install</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>join</servlet-name>
        <url-pattern>/join</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>WebSocket</servlet-name>
        <url-pattern>/websocket</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EmailAvailable</servlet-name>
        <url-pattern>/email_available</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <resource-ref>
        <description>My DataSource Reference</description>
        <res-ref-name>jdbc/db</res-ref-name>
        <res-type>javax.sql.DataSource</res-type>
        <res-auth>Container</res-auth>
    </resource-ref>
</web-app>
